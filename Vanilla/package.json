{
  "name": "vveplus-sample",
  "version": "1.0.0",
  "description": "This is a sample on how to use Video Express Plus ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm install --production=false && parcel build *.html --no-cache --no-minify --no-content-hash",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "server": "node index.js",
    "watch": "concurrently \"parcel watch *.html\" \"npm run server\""
  },
  "staticFiles": {
    "staticPath": [
      "assets/img"
    ],
    "watcherGlob": "**"
  },
  "keywords": [
    "VideoExpressPlus",
    "Breakout",
    "whiteboard",
    "waiting",
    "room"
  ],
  "author": "Rahulkumar Gaddam",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "babel-jest": "^28.0.3",
    "concurrently": "^7.3.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.0.3",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-static-files-copy": "^2.6.0"
  },
  "dependencies": {
    "@vonage/video-express": "^1.3.1",
    "@vonage/video-express-plus-client": "^1.3.1-beta.6",
    "@vonage/video-express-plus-server": "1.2.1-beta.3",
    "axios": "^0.25.0",
    "dotenv": "^16.0.1",
    "express": "^4.17.2",
    "express-sslify": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "lit-element": "^3.2.0",
    "regenerator-runtime": "^0.13.9",
    "sample": "^0.0.2",
    "uuid": "^8.3.2"
  }
}
